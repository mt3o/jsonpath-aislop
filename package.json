{
  "name": "jsonpath-rewritten",
  "version": "1.0.0",
  "description": "Rewrite of the https://www.npmjs.com/package/jsonpath library fixing prototype pollution CVE",
  "keywords": ["jsonpath", "json", "path", "query", "typescript"],
  "license": "BSD-2-Clause",
  "author": "mt3o",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsonpath-rewritten/jsonpath-modern"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "powershell -NoProfile -Command \"Remove-Item -Recurse -Force dist -ErrorAction SilentlyContinue\"",
    "test": "npm run build && node dist/test.js",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.6.3"
  }
}
